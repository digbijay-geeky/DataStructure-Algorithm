import java.util.*;
  
  public class Main{
  
  public static void main(String[] args) {
    // write your code here  
    Scanner scan=new Scanner(System.in);
    int n=scan.nextInt();
    int sum=0;
    while(n>0){
        int x=n%10;
        n=n/10;
        sum++;
    }
    System.out.println(sum);
   }
  }
