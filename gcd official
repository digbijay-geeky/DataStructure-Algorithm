import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) {
      // write your code here  
      Scanner scan=new Scanner(System.in);
      int a=scan.nextInt();
      int b=scan.nextInt();
      int x=a;
      int y=b;
      while(x!=0){
          int z=y%x;
           y=x;
           x=z;
         
          
          
      }
      System.out.println(y);
     }
    }
